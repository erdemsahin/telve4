<?xml version="1.0" encoding="UTF-8"?>
<job id="BatchLogCleaner" xmlns="http://xmlns.jcp.org/xml/ns/javaee" version="1.0">
    <properties>
        <property name="expireInterval" value="10" />
        <property name="hede" value="true" />
        <property name="bisi" value="FALSE" />
    </properties>
    <listeners>
        <listener ref="BatchStateListener"/>
    </listeners>
    <step id="process">
        <batchlet ref="BatchLogCleanerBatchlet">
        </batchlet>
    </step>
</job>